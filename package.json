{
  "name": "ng4-material-dropdown",
  "version": "1.0.0",
  "description": "Angular 4 material-like dropdown component",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc && npm run moveassets && npm run ngc && webpack --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --config webpack.demo.js --inline --colors --progress --port 3005 --content-base demo",
    "start": "npm run server",
    "test": "karma start karma.conf.js --single-run",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "moveassets": "rsync -a --include='*.scss' --include='*.js.map' --include='*.map' --include='*.html' --include='*/' --exclude='*' ./src/ ./dist/src"
  },
  "author": {
    "name": "Eric sites",
    "email": "eric_sites@mac.com"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.11",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "3.1.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.27.3",
    "es6-promise": "4.1.0",
    "es6-shim": "0.35.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "jasmine": "^2.5.3",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "sass-loader": "^6.0.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.16.0",
    "svg-url-loader": "^2.0.2",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "^3.0.2",
    "tslint": "^4.3.1",
    "typescript": "^2.2.1",
    "typings": "~2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.5"
  },
  "dependencies": {},
  "keywords": [
    "angular 4",
    "angular 4 material dropdown",
    "angular 4 component dropdown",
    "angular 4 dropdown",
    "angular 4 menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EricSites/ng4-material-dropdown"
  },
  "bugs": {
    "url": "https://github.com/EricSites/ng4-material-dropdown/issues"
  },
  "homepage": "https://github.com/EricSites/ng4-material-dropdown",
  "main": "./dist/ng4-dropdown.bundle.js",
  "files": [
    "dist",
    "tsconfig.json",
    "tsconfig.aot.json",
    "index.ts"
  ],
  "exclude": [
    "**/*.spec.ts"
  ]
}
